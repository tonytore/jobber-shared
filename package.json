{
  "version": "0.0.1",
  "license": "MIT",
  "main": "index.js",
  "description": "Helpers library for Jobber app",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && tsc --outDir build/src && node script/build-package.js",
    "build:cjs": "NODE_ENV=production BABEL_ENV=cjs babel src --presets=./script/babel-preset.js --extensions .ts,.tsx --ignore src/**/*.specs.tsx --out-dir build/cjs --source-maps",
    "build:esm": "NODE_ENV=production BABEL_ENV=esm babel src --presets=./script/babel-preset.js --extensions .ts,.tsx --ignore src/**/*.specs.tsx --out-dir build/esm --source-maps"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "keywords": [],
  "name": "@tonytore/jobber-shared",
  "author": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonytore/jobber-shared"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./icons": {
      "import": "./esm/icons/index.js",
      "require": "./cjs/icons/index.js"
    }
  },
  "types": "./src/index.d.ts",
  "dependencies": {
    "cloudinary": "^2.8.0",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2",
    "typescript": "^5.9.3",
    "winston": "^3.18.3",
    "winston-loki": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/express": "^5.0.4",
    "@types/jsonwebtoken": "^9.0.10"
  }
}
